<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.spring.my.SelldetailMapper">
 	<!-- ctrl shift y 소문자 x 대문자 -->
 	<!-- insert는 결제를 하면서 받아올때 -->
 	<!-- 판매추가 -->
 	<insert id="insert">
 		INSERT INTO SELLDETAIL(SCODE,PCODE,SELLCNT,AMOUNT) 
 		VALUES(#{scode},#{pcode},#{sellcnt},#{amount})
 	</insert>
</mapper>